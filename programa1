        cout << "La suma es: " << d1 + d2 << endl 
    }
#include<iostream>
int main()
{
    cout<<"\n         FIBONACCI \n\n";
    
    int i, num ;
    
    do
    {
        cout<<"Ingrese un numero entero y positivo: ";
        cin>>num;
        
    } while(num < 0);
    
    cout<<"\nLa serie es: \n\n\t";
    
    for(i=0; i<num; i++)
    {
       if(fibo(i) != 0)
          cout<< ", "; 
        
        cout<< fibo(i);       
    }
    
    system("pause");
    
    return 0;
}